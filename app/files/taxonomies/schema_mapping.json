{
  "$schema": "http://json-schema.org/schema#",
  "title": "Validator for the mapping in misp-taxonomies",
  "id": "https://www.github.com/MISP/misp-taxonomies/schema_mapping.json",
  "type": "object",
  "additionalProperties": false,
  "patternProperties": {
    "^.*$": {
      "properties": {
        "values": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
